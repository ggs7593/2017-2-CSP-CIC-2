CREATE TABLE ASYSARCHIVEVOLUME
(
  ARCHIVEID  CHAR(16 BYTE)                      NOT NULL,
  VOLUMEID   VARCHAR2(64 BYTE)                  NOT NULL,
  FILLORDER  INTEGER                            NOT NULL
)
TABLESPACE ASYSDEFS
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOLOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING
/


CREATE TABLE ASYSVOLUME
(
  VOLUMEID      VARCHAR2(64 BYTE)               NOT NULL,
  DESCR         VARCHAR2(64 BYTE)               NOT NULL,
  MAXSPACE      NUMBER                          NOT NULL,
  SPACELEFT     NUMBER                          NOT NULL,
  RECOVERSPACE  INTEGER                         NOT NULL,
  STATUS        INTEGER                         NOT NULL,
  ADMINSCLASS   CHAR(16 BYTE)                   NOT NULL,
  PARAMETER     VARCHAR2(64 BYTE),
  USABLESPACE   NUMBER
)
TABLESPACE ASYSDEFS
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOLOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING
/


--
-- PK_ASYSVOLUME  (Index) 
--
CREATE UNIQUE INDEX PK_ASYSVOLUME ON ASYSVOLUME
(VOLUMEID)
NOLOGGING
TABLESPACE ASYSDEFS
PCTFREE    10
INITRANS   2
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOPARALLEL
/


Insert into ASYSVOLUME
   (VOLUMEID, DESCR, MAXSPACE, SPACELEFT, RECOVERSPACE, 
    STATUS, ADMINSCLASS, PARAMETER, USABLESPACE)
 Values
   ('2HS_V001', '2HS_V001', 50000000000, 32888904764, 0, 
    1, 'NONE', NULL, 50000000000);
Insert into ASYSVOLUME
   (VOLUMEID, DESCR, MAXSPACE, SPACELEFT, RECOVERSPACE, 
    STATUS, ADMINSCLASS, PARAMETER, USABLESPACE)
 Values
   ('1HS_V001', '1HS_V001', 50000000000, 25972457554, 0, 
    1, 'NONE', NULL, 50000000000);

COMMIT;

Insert into ASYSARCHIVEVOLUME
   (ARCHIVEID, VOLUMEID, FILLORDER)
 Values
   ('1HS_ARC', '1HS_V001', 1);
Insert into ASYSARCHIVEVOLUME
   (ARCHIVEID, VOLUMEID, FILLORDER)
 Values
   ('2HS_ARC', '2HS_V001', 1);
COMMIT;
