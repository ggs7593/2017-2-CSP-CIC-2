CREATE TABLE ASYSCONTENTELEMENT
(
  ELEMENTID       CHAR(16 BYTE)                 NOT NULL,
  VOLUMEID        VARCHAR2(64 BYTE)             NOT NULL,
  FILEKEY         VARCHAR2(255 BYTE)            NOT NULL,
  FILESIZE        INTEGER                       NOT NULL,
  CREATEDATE      DATE                          NOT NULL,
  LASTACCESS      DATE                          NOT NULL,
  CONTENTTYPE     INTEGER                       NOT NULL,
  CONTENTCLASSID  CHAR(16 BYTE),
  GENERALDATA     VARCHAR2(64 BYTE),
  CHECKHINT       INTEGER,
  WRITEDATE       DATE,
  RETENTION       INTEGER,
  CHECKSUM        VARCHAR2(64 BYTE)
)
TABLESPACE ASYSCONTENT
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOLOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING
/


--
-- ASYSCE_CCLASSID_ELEMENTID  (Index) 
--
CREATE INDEX ASYSCE_CCLASSID_ELEMENTID ON ASYSCONTENTELEMENT
(CONTENTCLASSID, ELEMENTID)
NOLOGGING
TABLESPACE ASYSELEMENTS
PCTFREE    10
INITRANS   2
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOPARALLEL
/


